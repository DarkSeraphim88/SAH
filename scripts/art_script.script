function art_script:letsprocess()
	if (db.actor:money()<summ_final) then                                               ---------------- Проверка на наличие денег.
        self["Static_Des"]:TextControl():SetText(GetString("trans_no_money"))
		CORE = 1
	elseif (db.actor:money()>=summ_final) then
		local artcheked = {1,0,0,0,0}
		local recept_p = 0
		level[2] = 0
		level[3] = 0
		level[4] = 0
		level[5] = 0
        stop = 0
                                                                      --- Так как у нас 5 возможнных вариантов на один арт для симбиотов, сделаем цикл,
																	  --- где "firstart" будет строкой, в которой проверяем рецепт.
for firstart = 1,10 do                                
		if resultarttype[VTYPE] ~= 1 and firstart > 1 then                   --- Блочим повторную проверку для "не симбиотных" рецептов.
			break
		end
	if stop ~= 1 then
    artcheked = {1,0,0,0,0}
		if firstart ~= 1 then
			recept_af_check_const = recept_af_check_const+11
		end
			for p = 2,5 do                                            --- Цикл для проверки всех контейнеров.		
				    recept_p = recept_af_check_const+p-1              --- Значение необходимого артефакта в возможной строке.
						        -- Проверка на ненужность арта
						if recept[recept_p] == nil and art[p] == nil and artcheked[p] == 0 then
								artcheked[p] = 1
								level[p] = 0
								-- Проверка на симбиоты/катализаторы
							elseif recept[recept_p] ~= nil and art[p] ~= nil and artcheked[p] == 0 then	   
								for levels = 1,4 do
									if art[p] == recept[recept_p]..levels then
										artcheked[p] = 1
										level[p] = levels
										levelmax = levelmax + 4
										break
									end
								end
								-- Проверка на нерожденные
								if recept[recept_p] ~= nil and art[p] ~= nil and artcheked[p] == 0 then	   
									for types = 1,4 do
											if art[p] == recept[recept_p]..types then
												artcheked[p] = 1
												level[p] = 0
												break
							     	    	end
									end
								end
								-- Проверка на простые
								if recept[recept_p] ~= nil and art[p] ~= nil and artcheked[p] == 0 then	   
									for types = 1,9 do
										for levels = 1,4 do
											if art[p] == recept[recept_p]..types..levels then
												artcheked[p] = 1
												level[p] = levels
												levelmax = levelmax + 4
												break
							     	    	end
										end
									end
								end
								-- Проверка на аномальные симбиоты
							if recept[recept_p] ~= nil and art[p] ~= nil and artcheked[p] == 0 then	   
								for types = 0,9 do
								if artcheked[p] == 0 then
								    for levels = 1,4 do
									if artcheked[p] == 0 then
										for anomaly = 1,9 do
										if artcheked[p] == 0 then
											if art[p] == recept[recept_p]..types..levels..anomaly then
												artcheked[p] = 1
												level[p] = levels
												levelmax = levelmax + 4
												break
											end
										end
										end
									end
									end
								end
								end
							end
					end
			end
	else
		break
	end
			
----------------------- Проверка, совпал ли полностью рецепт. Если ДА - то дальше, если НЕТ - проверяем ещё 4 строки.


		
		if artcheked[1]==1 and artcheked[2]==1 and artcheked[3]==1 and artcheked[4]==1 and artcheked[5]==1 then
			stop = 1
			if resultart[VTYPE] == nil and resultarttype[VTYPE] == 1 then
			   resultart[VTYPE] = recept[recept_af_check_const+10]
			end
		else
			stop = 0
		end
		end
			
----------------------- Формируем уровень будущего артефакта, исходя из выбранных уровней.
            if stop == 1 then
				local level_min = 0
					for ll = 1,5 do
						if level[ll] ~= 0 then
							level_min = level_min+0.5
						end
					end

			level_final[VTYPE] = level[1]+level[2]+level[3]+level[4]+level[5]
			if  level_final[VTYPE]>levelmax-level_min then
				level_final[VTYPE] = 4
			elseif level_final[VTYPE]>=levelmax-level_min*2 then
				level_final[VTYPE] = 3
			elseif level_final[VTYPE]>=levelmax-level_min*4 then
				level_final[VTYPE] = 2
			else
				level_final[VTYPE] = 1
			end

		if resultarttype[VTYPE] == 1 or resultarttype[VTYPE] == 3 then
		    name_changing[VTYPE] = 0
		else
		    name_changing[VTYPE] = 1
		end

			
----------------------- Финальная проверка, проверка на кол-во повторов.
         
			if artcheked[1] == 1 and artcheked[2] == 1 and artcheked[3] == 1 and artcheked[4] == 1 and artcheked[5] == 1 then
			    local checked_count = {0,0,0,0,0}                                   --- Проверенные кол-ва необходимых артов в контейнерах.
		        local needcount = 1                                                 --- Кол-во необходимых артов.
					for counts = 1,5 do                                             --- Номер контейнера.
						needcount = recept[recept_af_check_const+10-6+counts]       --- Определяем нужное значение в строке.
							if art[counts] ~= nil then                              --- Если арт не пустой.
								db.actor:iterate_inventory(function (dummy, item) if item:section()==art[counts] then  count=count+1 end end, nil)      --- Функция расчета кол-во артов данного типа у ГГ.
									if count>=needcount then   
										checked_count[counts] = 1
									else
										checked_count[counts] = 0
									end
							elseif art[counts] == nil then
								checked_count[counts] = 1
							end
					end
					if checked_count[1] == 1 and checked_count[2] == 1 and checked_count[3] == 1 and checked_count[4] == 1 and checked_count[5] == 1 then
                        dialogs.relocate_money(npc,summ_final,"out") 
                        db.actor:give_info_portion("resultart_timer_start_"..VTYPE)
                        self:deletearts()
					else
					    self["Static_Des"]:TextControl():SetText(GetString("trans_no_arts"))
                        CORE = 1	
                        self:reset()
					end
				end
				
	elseif stop == 0 then
		resultart[VTYPE] = nil	
		dialogs.relocate_money(npc,summ_final,"out") 
		db.actor:give_info_portion("resultart_timer_start_"..VTYPE)
		self:deletearts()
	end
	end
end
