function save(packet)
    packet:w_u8(research[1])
	packet:w_u8(research[2])
	packet:w_u8(research[3])
	packet:w_u8(research[4])
	packet:w_u8(research[5])
	packet:w_u8(level_final[1])
	packet:w_u8(level_final[2])
	packet:w_u8(level_final[3])
	packet:w_u8(level_final[4])
	packet:w_u8(level_final[5])
	packet:w_u8(resultarttype[1])
