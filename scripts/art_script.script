	art = {}
	level_choosed = 0
	level_choosed_yach = {0,0,0,0,0}
	levelmax = 0
	deleted_arts[VTYPE*5-4] = 0
	deleted_arts[VTYPE*5-3] = 0
	deleted_arts[VTYPE*5-2] = 0
	deleted_arts[VTYPE*5-1] = 0
	deleted_arts[VTYPE*5] = 0
	research[VTYPE] = 0
	resultart[VTYPE] = nil
	resultarttype[VTYPE] = 0
	level_final[VTYPE] = 0
	firstclick = 0
	recept_mode_on = 0
	self:reset()
