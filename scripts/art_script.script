function art_script:deletearts()
	for n = 1,6 do
		if art[n] ~= nil and n ~= 6 then
			deleted_arts[VTYPE*5+n-5] = art[n]
	        xr_effects.remove_item(actor, npc, {art[n]})
			art[n] = nil
			summ[n] = 0
		elseif n == 6 then
	        xr_effects.remove_item(actor, npc, {art[n]})
			art[n] = nil
			summ[n] = 0
		end
	end
	self:reset()
end

