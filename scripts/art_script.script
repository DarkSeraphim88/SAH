                                                    level[1] = w
                                                    levelmax = levelmax + 4
                                                    self:letsprocess()
                                                    break
										    	end
							                if stop == 0 then
									            for l = 0,9 do
            		                                    if art[1] == recept[recept_af_check_const]..w..l and stop == 0 then
                                                            resultart[VTYPE] = recept[recept_af_check_const+10]
                                                            resultarttype[VTYPE] = u
                                                            linetype = i
                                                            stop = 1
                                                            level[1] = w
                                                            levelmax = levelmax + 4
                                                            self:letsprocess()
                                                            break
													    end
												end
				                            else
				                                break
				                            end
						                end
				            else
				                break
				            end
						end
	  	        elseif u == 3 then
                    recept = recept_abs
		                for i = 1,36 do
